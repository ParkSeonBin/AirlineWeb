USE odbo

// 승객 로그인 정보
CREATE TABLE members (
id varchar(20) not null,
passwd varchar(20),
email varchar(50),
);

// 항공사가 제공하는 항공편 정보
CREATE TABLE airtime (
arrival varchar(20) not null,
depart varchar(10) not null
);

// 승객이 예매한 티켓 정보
CREATE TABLE ticket (
id varchar(20) not null,
arrival varchar(20),
depart varchar(10)
);
